{
  "name": "electron-js-app",
  "productName": "electron-js-app",
  "version": "0.1.0",
  "description": "Electron Application using only JS",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "keywords": [],
  "author": {
    "name": "Victor Quiroz",
    "email": "victorhqc@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.5",
    "css-loader": "^3.6.0",
    "electron": "9.1.2",
    "electron-rebuild": "^1.11.0",
    "electron-redux": "^1.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-loader": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "sqlite3": "^5.0.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.6.0",
    "ts-loader": "^6.2.2",
    "typed-redux-saga": "^1.1.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
